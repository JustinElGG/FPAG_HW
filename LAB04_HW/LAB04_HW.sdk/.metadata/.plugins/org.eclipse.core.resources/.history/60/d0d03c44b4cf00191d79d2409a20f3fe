

/***************************** Include Files *******************************/
#include "arithemetic.h"

/************************** Function Definitions ***************************/
u32 add(UINTPTR baseAddr, u32 A, u32 B, u32 operator) {
	u32 ans;
	u32 data = A + (B << 8);
	ADDER_mWriteReg(baseAddr, 0, data);
	ans = ADDER_mReadReg (baseAddr, 4);
	return ans;
}
